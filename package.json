{
  "name": "@maja.id/nestjs-oauth2-server",
  "version": "4.0.2",
  "description": "A Nestjs wrapper for oauth2-server",
  "main": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "author": "Prastowo Agung Widodo <agung@makaramas.com>",
  "keywords": [
    "nestjs oauth2 server",
    "nestjs oauth2",
    "@nestjs/oauth2-server",
    "nest oauth2 server",
    "nestjs",
    "nest"
  ],
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "format": "prettier --write lib/**/*.ts tests/**/*.ts",
    "prebuild": "rimraf -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "rimraf -rf dist && tsc -p tsconfig.json -w",
    "lint": "eslint lib/**/*.ts --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test": "npm run test:integration",
    "test:integration": "jest --config ./tests/jest-e2e.config.json --runInBand",
    "test:cov": "jest --config ./tests/jest-e2e.config.json --runInBand --coverage",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@nestjs/common": "10.3.9",
    "@nestjs/core": "10.3.9",
    "@nestjs/platform-express": "10.3.9",
    "@nestjs/testing": "10.3.9",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.1",
    "@types/oauth2-server": "3.0.16",
    "@types/rimraf": "3.0.2",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "dotenv-cli": "7.4.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "jest": "29.7.0",
    "lint-staged": "15.2.5",
    "moment": "2.30.1",
    "oauth2-server": "3.1.1",
    "prettier": "3.3.0",
    "reflect-metadata": "0.2.2",
    "release-it": "17.3.0",
    "renovate": "37.388.1",
    "rimraf": "5.0.7",
    "rxjs": "7.8.1",
    "supertest": "7.0.0",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5"
  },
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "oauth2-server": "3.1.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
